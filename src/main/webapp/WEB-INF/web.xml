<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>

	<display-name>Archetype Created Web Application</display-name>
	
	<servlet>
		<servlet-name>WxServlet</servlet-name>
		<servlet-class>com.company.main.WxServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>WxWebAuthorizeServlet</servlet-name>
		<servlet-class>com.company.main.WxWebAuthorizeServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>WxServlet</servlet-name>
		<url-pattern>/WxServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>WxWebAuthorizeServlet</servlet-name>
		<url-pattern>/authorization</url-pattern>
	</servlet-mapping>
	
</web-app>
